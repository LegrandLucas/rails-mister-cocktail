<h1>Add a new cocktail to our list!</h1>

<%= simple_form_for @cocktail do |f| %>
  <%= f.input :name %>
  <%= f.button :submit, class: "btn btn-primary"%>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
<% end %>
<%= link_to "Home Page", cocktails_path, class: "btn btn-warning"%>
